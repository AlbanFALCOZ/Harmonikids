    "@types/canvas-confetti": "^1.6.4",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^20.14.5",
    "copyfiles": "^2.4.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "playwright": "^1.34.3",
    "typescript": "~5.3.3"
  }
